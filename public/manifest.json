{
  "manifest_version": 2,
  "name": "Transcript Search",
  "version": "1.0",

  "description": "Searches youtube videos for words",

  "icons": {
    "48": "icons/border-48.png"
  },
  "browser_action": {
    "default_popup": "index.html"
  },

  "content_scripts": [
    {
      "matches": ["https://*.youtube.com/*"],
      "exclude_matches": ["https://accounts.youtube.com/RotateCookiesPage*"]
    }
  ]
}
